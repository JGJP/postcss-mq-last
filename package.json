{
  "name": "postcss-mq-last",
  "version": "2.0.0",
  "description": "PostCSS plugin that gives media query rules precedence by moving them to the end of the file",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "media",
    "query",
    "last",
    "precedence"
  ],
  "author": "J <j@jgarcia.jp>",
  "license": "MIT",
  "repository": "jgjp/postcss-mq-last",
  "bugs": {
    "url": "https://github.com/jgjp/postcss-mq-last/issues"
  },
  "homepage": "https://github.com/jgjp/postcss-mq-last",
  "peerDependencies": {
    "postcss": "^8.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "postcss": "^8.5.6"
  },
  "scripts": {
    "test": "jest && eslint *.js"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "jest": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "single"
      ]
    }
  }
}
